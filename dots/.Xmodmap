! Turn Print to the Super_R
clear mod4
add mod4 = Super_L Super_R 
keycode 107 = Super_R Print Print NoSymbol

! Switch back Ctrl and Alt
clear control
clear mod1
keycode 37 = Control_L
keycode 64 = Alt_L Meta_L 
keycode 105 = Control_R 

! CapsLock to Control 
clear lock
keycode 66 = Control_L NoSymbol Caps_Lock NoSymbol

! RightAlt to Mode switch
clear mod5
keycode 108 = Mode_switch Meta_R NoSymbol NoSymbol

! Mode_switch + B,N,P,F to Arrow
keycode 56 = b B Left NoSymbol
keycode 57 = n N Down NoSymbol
keycode 33 = p P Up NoSymbol
keycode 41 = f F Right NoSymbol

add control = Control_L Control_R
add mod1 = Alt_L Alt_R
add mod5 = Mode_switch
